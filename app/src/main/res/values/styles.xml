<resources>

    <style name="Widget.GetApk.AppBarLayout" parent="Widget.Material3.AppBarLayout"/>

    <style name="Widget.GetApk.Toolbar" parent="Widget.Material3.Toolbar"/>

    <style name="Widget.GetApk.PopupMenu.Overflow" parent="Widget.Material3.PopupMenu.Overflow">
        <item name="android:overlapAnchor">false</item>
    </style>

    <style name="TextAppearance.GetApk.Widget.ActionBar.Menu" parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.GetApk.InfoItemView" parent="">
        <item name="titleTextAppearance">@style/TextAppearance.GetApk.Title</item>
        <item name="textAppearance">@style/TextAppearance.GetApk.Text</item>
        <item name="titleMargin">6dp</item>
    </style>

    <style name="TextAppearance.GetApk.Title" parent="TextAppearance.Material3.TitleSmall"/>

    <style name="TextAppearance.GetApk.Text" parent="TextAppearance.Material3.BodySmall"/>
